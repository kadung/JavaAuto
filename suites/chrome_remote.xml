<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="classes" thread-count="1">
	<!-- Supported browser: chrome, firefox, ie, safari -->
	<parameter name="browserName" value="safari"/>
	
	<!-- Use grid: y or n -->
	<parameter name="remote" value="y"/>
				
	<!-- URL of grid hub -->
	<parameter name="gridURL" value="http://192.168.1.103:4444/wd/hub"/>
					
	<!-- selected OS Flatform: https://github.com/SeleniumHQ/selenium/wiki/Grid-Platforms -->
	<parameter name="desiredPlatform" value="MAC"/>
			
	<!-- selected browser version -->
	<parameter name="browserVersion" value=""/>			
	
    <test name="Test">
        <classes>
            <class name="example.Test1" />
        </classes>
    </test>
</suite>