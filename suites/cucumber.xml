<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<!-- parallel: which type runs parallel (methods/classes/HelloController/instance)
	 thread-count: is number of concurrent thread running -->
<suite name="Suite" parallel="classes" thread-count="1">
    <listeners>
        <listener class-name="listeners.RetryListener"/>
    </listeners>

	<!-- Supported browser: chrome, firefox, ie, safari -->
	<parameter name="browserName" value="chrome"/>
	<!-- Use grid: y or n -->
	<parameter name="remote" value="n"/>
	<!-- URL of grid hub -->
	<parameter name="gridURL" value=""/>
	<!-- selected OS Flatform: https://github.com/SeleniumHQ/selenium/wiki/Grid-Platforms -->
	<parameter name="desiredPlatform" value=""/>
	<!-- selected browser version -->
	<parameter name="browserVersion" value=""/>			


    <test name="Test" annotations="JDK" preserve-order="true">
        <classes>
            <class name="cucumber.TestRunner" />
        </classes>
    </test>
</suite>